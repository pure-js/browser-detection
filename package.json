    "@typescript-eslint/parser": ">=6.0.0",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": ">=8.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "typescript": "^5.1.6",
